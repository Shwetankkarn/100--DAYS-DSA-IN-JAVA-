public class Solution {
    public ListNode detectCycle(ListNode head) {
        ListNode slow= head;
        ListNode fast= head;
        while(fast!=null && fast.next!=null){
            slow= slow.next;
            fast= fast.next.next;

            if(slow==fast){
                ListNode temp= head;
                int idx=0;
                while(temp!=slow){
                    slow= slow.next;
                    temp= temp.next;
                    idx++;
                    
                       
        }
         return temp;
        
        }
       
    }
     return null;
}
}
