class Solution {
    
    public int power(int a, int b){
        if(b==0) return 1;
        int call= power(a,b/2);
        if(b%2==0) return call*call;
        else return call*call*a;
    }
    
    public int reverseExponentiation(int n) {
     
     if(n==10) return 10;
     return power(n,n);
    }
}
